Package: FlowRepositoryQuery
Title: Query on FlowRepository experiments
Version: 0.1.0
Authors@R: c(
    person("EugÃ©nie", "Lohmann", email ="eugenie.lohmann@inserm.fr", role = c("aut", "cre")),
    person("Samuel", "Granjeaud", email = "samuel.granjeaud@inserm.fr", role = "aut")
